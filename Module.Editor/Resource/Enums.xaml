<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:inf="clr-namespace:FomodInfrastructure;assembly=FomodInfrastructure">

    <inf:Set x:Key="compositeDependencyOperator" x:Shared="false">
        <sys:String>And</sys:String>
        <sys:String>Or</sys:String>
    </inf:Set>


    <inf:Set x:Key="flagValueEnum" x:Shared="false">
        <sys:String>On</sys:String>
        <sys:String>Off</sys:String>
    </inf:Set>

</ResourceDictionary>  