<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">


    <Thickness x:Key="Margin" Right="6" Top="3" Left="6" Bottom="3"/>
    <Thickness x:Key="Pading" Right="12" Top="6" Left="12" Bottom="6"/>
    <SolidColorBrush x:Key="Foreground" Color="White"/>
    <SolidColorBrush x:Key="BorderIsMouseOver" Color="#FF7EB4EA"/>
    <SolidColorBrush x:Key="BackgroundIsFocused" Color="#FF23486E"/>
    <CornerRadius x:Key="CornerRadius"  TopLeft="1" TopRight="1" BottomRight="1" BottomLeft="1"/>
    <sys:Double x:Key="FontSize">14</sys:Double>
    <GridLength x:Key="GridColumn_1_Width">150</GridLength>
</ResourceDictionary>